{
  "version": 2,
  "builds": [
    {
      "src": "web-build",
      "use": "@vercel/static"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/games",
      "dest": "./routes/gamesRoutes.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    }
  ]
}
